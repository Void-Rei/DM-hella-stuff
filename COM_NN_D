void COM_NN_D() //сравнение двух натуральных чисел
{
	int n1, n2;
	cout << "Введите два натуральных числа" << endl;
	cin >> n1;	
	if (n1 <= 0) cout << "Число " << n1 << " не является натуральным";		//проверка на натуральность первого числа
	else 
	{
		cin >> n2;
		if (n2 <= 0) cout << "Число " << n2 << " не является натуральным";	//проверка на натуральность второго числа
		else
		{
			if (n1 > n2) cout << "2";
			if (n1 == n2) cout << "0";
			if (n1 < n2) cout << "1";
		}
	}
}
